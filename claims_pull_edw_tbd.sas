               CREATE TABLE &ORA_TMP..&TABLE_PREFIX._NDC_RE AS
               SELECT * 
               FROM &ORA_TMP..&TABLE_PREFIX._NDC_RX)BY ORACLE;
      		DISCONNECT FROM ORACLE;
      		QUIT;
 
		%END;

		%EDW_CLAIMS(ADJ_ENGINE = RE);
		
		%IF %SYSFUNC(EXIST(&ORA_TMP..ALGN_LVL_LIST_&INITIATIVE_ID._RE)) %THEN %DO;
		  %PUT NOTE:  DROP TABLE RECAP CC:  &ORA_TMP..ALGN_LVL_LIST_&INITIATIVE_ID._RE ;
		  %DROP_ORACLE_TABLE(TBL_NAME=&ORA_TMP..ALGN_LVL_LIST_&INITIATIVE_ID._RE);
		%END;

		/*  New RP  */

		%IF (&PROGRAM_ID EQ 105 OR (&PROGRAM_ID EQ 106 AND &TASK_ID EQ 21)) %THEN %DO;


                     %IF (&PROGRAM_ID EQ 105) %THEN %DO;
                       %IF %SYSFUNC(EXIST(&ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._RE)) %THEN %DO;
                       
                           PROC SQL NOPRINT;
                           SELECT COUNT(*) INTO: RE_BEFORE_COUNT
                           FROM &ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._RE; 
                           QUIT;

                           %PUT NOTE:  RECAP BEFORE COUNT = &RE_BEFORE_COUNT.;   
                           
                           %IF &RE_BEFORE_COUNT. > 0 %THEN %DO;
                     
                                PROC SQL;
                                CONNECT TO ORACLE(PATH=&GOLD);
                                EXECUTE ( 
                                DELETE FROM &ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._RE a
                                WHERE CLIENT_LEVEL_1 in (
                                                          SELECT DISTINCT INSURANCE_CD
                                                          FROM &RESOLVE_CLIENT_TABLE_RE_MIGR. )
                                ) BY ORACLE;
                                DISCONNECT FROM ORACLE;
                                QUIT;
                                
                           %END;
                                
                           PROC SQL NOPRINT;
                           SELECT COUNT(*) INTO: RE_AFTER_COUNT
                           FROM &ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._RE; 
                           QUIT;

                           %PUT NOTE:  RECAP AFTER COUNT = &RE_AFTER_COUNT.;                                
                     
                       %END;
                     %END;

			%DROP_ORACLE_TABLE(TBL_NAME=&ORA_TMP..CLAIMS_PULL_&initiative_id._R2X);

		
	  		DATA _NULL_;
	                 CALL SYMPUT('chk_dt',TODAY()); 
	                 CALL SYMPUT('chk_dt_oracle',"'" || PUT(TODAY(), yymmdd10.) || "'"); 
	        RUN;

			%put NOTE: chk_dt_oracle = &chk_dt_oracle. ;

			PROC SQL;
			CONNECT TO ORACLE(PATH=&GOLD PRESERVE_COMMENTS);            
			CREATE TABLE &ORA_TMP..CLAIMS_PULL_&initiative_id._R2X AS
			select * from connection to oracle
			(WITH HISTORY_MEMBERS AS (
			SELECT DISTINCT A.MBR_ID, A.MBR_GID, B.MBR_BRTH_DT, B.MBR_GNDR_GID, B.REL_CODE, 
			B.PAYER_ID, A.TRGT_PAYER_ID, B.PRSN_CURR_KEY, B.QL_BNFCY_ID, B.RPT_MBR_ID
			FROM  &ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._&ADJ_ENGINE._MIGR A,
			      DSS_CLIN.V_MBR B       
			WHERE  A.MBR_GID=B.MBR_GID
			AND A.PAYER_ID=B.PAYER_ID  
		 	),
		 
		 	CC_MEMBERS AS (
			select DISTINCT 
			'RX' as ADJ_ENGINE,
			C.ALGN_LVL_GID, 
			ALG.EXTNL_LVL_ID1 AS CLIENT_LEVEL_1,
			C.PAYER_ID,
			d.payer_id as payer_id2,
			C.MBR_GID,
			D.MBR_GID as MBR_GID2,
			C.ELIG_END_DT,
			D.MBR_ID as MBR_ID2,
			MBR.MBR_ID MBR_ID
			FROM   HISTORY_MEMBERS  D   		
			LEFT JOIN DSS_CLIN.V_MBR MBR                
			ON  D.QL_BNFCY_ID = MBR.QL_BNFCY_ID AND D.TRGT_PAYER_ID=MBR.PAYER_ID
			AND D.MBR_BRTH_DT=MBR.MBR_BRTH_DT 
			AND D.MBR_GNDR_GID=MBR.MBR_GNDR_GID 
			AND D.REL_CODE=MBR.REL_CODE
			AND MBR.SRC_SYS_CD='X'
			and d.QL_BNFCY_ID is not null 
			INNER JOIN        DSS_CLIN.V_MBR_ELIG_ACTIVE C      ON MBR.MBR_GID = C.MBR_GID                         
			LEFT JOIN         DSS_CLIN.V_ALGN_LVL_DENORM ALG    ON C.ALGN_LVL_GID = ALG.ALGN_LVL_GID_KEY  and ALG.SRC_SYS_CD = 'X' 
			
			union all
			
			SELECT  DISTINCT 
			'RX' as ADJ_ENGINE,
			C.ALGN_LVL_GID, 
			ALG.EXTNL_LVL_ID1 AS CLIENT_LEVEL_1,
			C.PAYER_ID,
			d.payer_id as payer_id2,
			C.MBR_GID,
			D.MBR_GID as MBR_GID2,
			C.ELIG_END_DT,
			D.MBR_ID as MBR_ID2,
			MBR.MBR_ID MBR_ID
			FROM   HISTORY_MEMBERS  D   		
			LEFT JOIN DSS_CLIN.V_MBR MBR                
			ON  D.PRSN_CURR_KEY = MBR.PRSN_CURR_KEY 
			AND D.TRGT_PAYER_ID=MBR.PAYER_ID
			AND D.MBR_BRTH_DT=MBR.MBR_BRTH_DT 
			AND D.MBR_GNDR_GID=MBR.MBR_GNDR_GID 
			AND D.REL_CODE=MBR.REL_CODE 	
			AND D.PRSN_CURR_KEY IS NOT NULL 
			INNER JOIN DSS_CLIN.V_MBR_ELIG_ACTIVE C      
			ON 
			MBR.MBR_GID = C.MBR_GID                         
			LEFT JOIN DSS_CLIN.V_ALGN_LVL_DENORM ALG   
			ON 
			C.ALGN_LVL_GID = ALG.ALGN_LVL_GID_KEY  and ALG.SRC_SYS_CD = 'X' 
			
			union all
			
			SELECT DISTINCT 
			'RX' as ADJ_ENGINE,
			C.ALGN_LVL_GID, 
			ALG.EXTNL_LVL_ID1 AS CLIENT_LEVEL_1,
			C.PAYER_ID,
			d.payer_id as payer_id2,
			C.MBR_GID,
			D.MBR_GID as MBR_GID2,
			C.ELIG_END_DT,
			D.MBR_ID as MBR_ID2,
			MBR.MBR_ID MBR_ID
			FROM   HISTORY_MEMBERS  D  	
			LEFT JOIN DSS_CLIN.V_MBR MBR                
			ON  D.RPT_MBR_ID = MBR.RPT_MBR_ID 
			AND D.TRGT_PAYER_ID=MBR.PAYER_ID
			AND D.MBR_BRTH_DT=MBR.MBR_BRTH_DT 
			AND D.MBR_GNDR_GID=MBR.MBR_GNDR_GID 
			AND D.REL_CODE=MBR.REL_CODE
			AND D.RPT_MBR_ID IS NOT NULL
			INNER JOIN        DSS_CLIN.V_MBR_ELIG_ACTIVE C      
			ON 
			MBR.MBR_GID = C.MBR_GID                         
			LEFT JOIN         DSS_CLIN.V_ALGN_LVL_DENORM ALG    
