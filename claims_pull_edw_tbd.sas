						,PRCTR.DEGR_1_CD
						,PRCTR.REC_SRC_FLG as prctr_rec_src
						,PRCTR.TERM_REAS_CD  as prctr_term_reas_cd
						,PHMCY.NABP_CODE_6
						,CLAIM.PAYER_ID
						,CLAIM.MBR_GID
