		%LET PAYER_ID_CONS = %STR(AND MBR.PAYER_ID&AVOID_IX_LKP. BETWEEN 500000 AND 2000000);
		%LET HIERARCHY_LIST = %STR(	,A.RPT_OPT1_CD AS CLIENT_LEVEL_1
					   	   			,A.EXTNL_LVL_ID1 AS CLIENT_LEVEL_2
					   	   			,A.EXTNL_LVL_ID3 AS CLIENT_LEVEL_3	);
		%LET GCN_CODE = %STR(CAST(DRUG.RECAP_GNRC_CLASS_NBR AS INT) AS GCN_CODE);
		%LET CARRIER_FIELD = CLIENT_LEVEL_2;
/*  New RP  */
		%LET CC_CREATE_CLAIMS_RE_COND = %STR(&ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._&ADJ_ENGINE. );		
		%LET CC_RE_MIGR_TBL = %STR(&RESOLVE_CLIENT_TABLE_RE);
		%LET CC_RE_ON_OFF=OFF;	
/*  New RP  */

		%EDW_CLAIMS(ADJ_ENGINE = RE);
	%END;

/*  New RP  */

	%PUT NOTE: CC_RE_MIGR_IND = &CC_RE_MIGR_IND. ;
	%PUT NOTE: RESOLVE TABLE = &RESOLVE_CLIENT_TABLE_RE_MIGR. ;
		
	%IF &CC_RE_MIGR_IND. = 1 %THEN %DO;
	 %IF %SYSFUNC(EXIST(&RESOLVE_CLIENT_TABLE_RE_MIGR.)) %THEN %DO;
		%LET ADJ_ENGINE= RE;
		%LET SRC_SYS_CD = %STR(R);
		%LET PAYER_ID_CONS = %STR(AND MBR.PAYER_ID&AVOID_IX_LKP. BETWEEN 500000 
		AND 2000000);
		%LET HIERARCHY_LIST = %STR(	,A.RPT_OPT1_CD AS CLIENT_LEVEL_1
									,A.EXTNL_LVL_ID1 AS CLIENT_LEVEL_2
									,A.EXTNL_LVL_ID3 AS CLIENT_LEVEL_3	);
/*		%LET REFILL_QTY = %STR(,VCLM.ATHZD_REFIL_QTY AS REFILL_FILL_QY);*/									
		%LET GCN_CODE = %STR(CAST(DRUG.RECAP_GNRC_CLASS_NBR AS INT) AS GCN_CODE);
		%LET CARRIER_FIELD = CLIENT_LEVEL_2;	
		%LET CC_CREATE_CLAIMS_RE_COND = %STR(&ORA_TMP..CLAIMS_PULL_&INITIATIVE_ID._&ADJ_ENGINE._MIGR ); 
		%LET CC_RE_MIGR_TBL = %STR(&RESOLVE_CLIENT_TABLE_RE_MIGR);
		%LET RESOLVE_CLIENT_TABLE_RE=%STR(&RESOLVE_CLIENT_TABLE_RE_MIGR); 
		%LET CC_RE_ON_OFF=ON;	

		%IF %SYSFUNC(EXIST(&ORA_TMP..&TABLE_PREFIX._NDC_RX))=1 %THEN %DO;         

      		%DROP_ORACLE_TABLE(TBL_NAME=&ORA_TMP..&TABLE_PREFIX._NDC_RE);

